{
  "name": "@fgrid-ngx/mde",
  "description": "Angular Material Extended",
  "version": "3.0.3",
  "author": {
    "name": "Joe Jordan Brown",
    "email": "joe.jordan.brown@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/material-extended/mde"
  },
  "bugs": {
    "url": "https://github.com/material-extended/mde/issues"
  },
  "keywords": [
    "angular",
    "angular popover",
    "material",
    "material design",
    "material extended",
    "material popover",
    "components",
    "popover"
  ],
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run build:library && npm run serve",
    "serve": "ng serve --aot",
    "build": "ng build",
    "build:library": "ng build @fgrid-ngx/mde",
    "build:library:prod": "ng build @fgrid-ngx/mde --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "release:beta": "standard-version --prerelease beta",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.12",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.3.12",
    "@angular/compiler": "^13.3.12",
    "@angular/core": "^13.3.12",
    "@angular/forms": "^13.3.12",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "^13.3.12",
    "@angular/platform-browser-dynamic": "^13.3.12",
    "@angular/router": "^13.3.12",
    "@fgrid-ngx/mde": "^3.0.3",
    "core-js": "^2.6.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.10",
    "@angular-devkit/build-ng-packagr": "~0.1002.4",
    "@angular/cli": "~13.3.10",
    "@angular/compiler-cli": "^13.3.12",
    "@angular/language-service": "^13.3.12",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "standard-version": "^8.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
